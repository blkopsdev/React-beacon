{
  "name": "beacon-m",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "@types/lodash": "^4.14.109",
    "@types/react-bootstrap": "^0.32.9",
    "@types/react-redux": "^6.0.0",
    "@types/react-redux-toastr": "^7.0.11",
    "@types/react-router-dom": "^4.2.6",
    "adal-angular": "^1.0.17",
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "lodash": "^4.17.10",
    "react": "^16.4.0",
    "react-adal": "git+https://github.com/jfbloom22/react-adal.git#master",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.0",
    "react-masonry-component": "^6.2.0",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.2.6",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.16.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,tsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "precommit": "lint-staged",
  "watch": {
    "build-css": {
      "patterns": [
        "src/**/*.less"
      ],
      "extensions": "less",
      "quiet": true
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "build-css": "lessc src/main.less src/index.css",
    "start-js": "react-scripts-ts start",
    "lint-staged": "lint-staged",
    "start": "npm-run-all -p watch start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/adal-angular": "^1.0.1",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "husky": "^0.14.3",
    "less": "^3.0.4",
    "lint-staged": "^7.1.2",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.3",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "typescript": "^2.8.3"
  }
}
